{
  "name": "baseball-frontend",
  "version": "1.0.0",
  "private": false,
  "description": "Baseball application frontend for team or player performance across a baseball season",
  "author": "Ethan T. Painter",
  "scripts": {
    "build": "tsc",
    "dev": "webpack-dev-server --mode development",
    "prod": "webpack --mode production",
    "clean": "rm -rf ./node_modules && rm -rf ./dist",
    "ci": "npm run clean && npm i && npm run lint && npm run build && npm run test",
    "lint": "eslint src/**/*.js && eslint src/**/*.tsx",
    "lint:src": "eslint src/**/*.tsx",
    "lint:test": "eslint test/**/*.spec.ts"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.3.0",
    "eslint": "^6.8.0",
    "mocha": "^8.1.3",
    "prettier": "^2.1.2",
    "source-map-loader": "^1.1.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.4",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "prettier": {
    "printWidth": 80,
    "useTabs": true,
    "tabWidth": 2,
    "arrowParens": "always",
    "bracketSpacing": true,
    "semi": true,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "trailingComma": "es5"
  },
  "bugs": {
    "url": "https://github.com/EthanTPainter/Baseball-Frontend/issues"
  },
  "homepage": "https://github.com/EthanTPainter/Baseball-Frontend#readme",
  "keywords": [
    "Typescript",
    "React",
    "Baseball",
    "Frontend"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EthanTPainter/Baseball-Frontend.git"
  }
}
